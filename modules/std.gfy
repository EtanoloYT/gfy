set __ZERO 0

define for(i,stop,callable):
    less i stop
        callable
        add i 1
        for i stop callable
    end
end

define >(a,b,callable):
    greater a b
        callable
    end
end

define <(a,b,callable):
    less a b
        callable
    end
end

define =(a,b,callable):
    equals a b
        callable
    end
end